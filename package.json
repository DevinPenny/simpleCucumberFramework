{
  "name": "automation-ui-reference",
  "version": "1.0.0",
  "description": "Cucumber UI test reference",
  "main": "server.js",
  "config": {
    "test": {
      "application": "reference",
      "environment": "qa",
      "retry": 0
    },
    "grid": {
      "useGrid": false,
      "gridUrl": "some-url",
      "parallel": "5"
    },
    "browser": {
      "closeOnFail": true,
      "browserName": "chrome"
    },
    "reporter": {
      "launchReport": true,
      "reportType": "single",
      "takeScreenShot": true
    },
    "testRail": {
      "testRailUrL": "test rail url goes here",
      "testRailProjectName": "Test Project",
      "updateTestRail": false,
      "closeRun": false
    },
    "jira": {
      "jiraUrL": "jira url goes here",
      "jiraProject": "project",
      "updateJira": false
    }
  },
  "scripts": {
    "lint": "eslint --ext=.js . || true",
    "jsdoc": "jsdoc . -c jsdoc-config.js",
    "gridUp": "docker-compose -H tcp://10.248.153.240:2376 --tls -f ./config/selenium-grid.yaml up -d --scale chrome=74 --scale firefox=1",
    "gridDown": "docker-compose -H tcp://10.248.153.240:2376 --tls -f ./config/selenium-grid.yaml down"
  },
  "keywords": [
    "Selenium",
    "Cucumber",
    "BDD",
    "Jira",
    "TestRail",
    "Axios",
    "Chai"
  ],
  "author": "Devin Penny <devin.penny@gmail.com>",
  "contributors": [
    "none"
  ],
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/html-formatter": "^18.0.0",
    "axios": "^0.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "latest",
    "cucumber-html-reporter": "^5.5.0",
    "eslint": "^8.10.0",
    "fs": "0.0.1-security",
    "geckodriver": "latest",
    "jira-client": "^8.0.0",
    "jsdoc": "^3.6.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "multiple-cucumber-html-reporter": "^1.19.0",
    "selenium-webdriver": "^4.1.1",
    "testrail-api": "^1.3.6",
    "uuid": "^8.3.2",
    "yargs": "^12.0.5"
  }
}
